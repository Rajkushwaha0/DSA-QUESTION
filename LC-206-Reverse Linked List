class Solution {
    public ListNode reverseList(ListNode head) {
        ListNode curr=head;
        ListNode perv = null;
        ListNode forward=null;
        while(curr!=null){
            forward=curr.next;
            curr.next=perv;
            perv=curr;
            curr=forward;
        }
        head=perv;
        return head;
    }
}
